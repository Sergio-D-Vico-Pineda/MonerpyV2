{
  "name": "monerpyv2",
  "type": "module",
  "version": "0.0.1",
  "author": {
    "name": "Scarpy19",
    "url": "https://github.com/Scarpy19"
  },
  "engines": {
    "node": "22"
  },
  "packageManager": "pnpm@10.5.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "init": "pnpm install && prisma generate",
    "dev": "astro dev",
    "deve": "astro dev --host",
    "build": "prisma generate && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "start": "prisma generate && node ./dist/server/entry.mjs",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@astrojs/node": "^9.4.0",
    "@libsql/client": "^0.15.10",
    "@prisma/adapter-libsql": "^6.13.0",
    "@prisma/client": "6.13.0",
    "@tailwindcss/vite": "^4.1.11",
    "astro": "^5.12.5",
    "tailwindcss": "^4.1.11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "esbuild",
      "prisma"
    ],
    "ignoredBuiltDependencies": [
      "sharp"
    ]
  },
  "devDependencies": {
    "prisma": "^6.13.0",
    "tsx": "^4.20.3"
  }
}